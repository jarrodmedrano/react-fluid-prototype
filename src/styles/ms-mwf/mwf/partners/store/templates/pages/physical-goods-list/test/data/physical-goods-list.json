                        },
                        "content": {
                            "text": "50 to 100"
                        }
                    },
                    {
                        "config": {
                            "selected": false,
                            "radio": true
                        },
                        "content": {
                            "text": "100 to 150",
                            "radio": true
                        }
                    },
                    {
                        "config": {
                            "selected": false,
                            "radio": true
                        },
                        "content": {
                            "text": "150 to 200"
                        }
                    }]
                }
            }]
        }
    },
