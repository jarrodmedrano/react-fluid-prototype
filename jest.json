{
  "snapshotSerializers": [
    "<rootDir>/node_modules/enzyme-to-json/serializer"
  ],
  "coveragePathIgnorePatterns": ["<rootDir>/jspm_packages", "<rootDir>/node_modules",  "<rootDir>/src/jspm_packages"],
  "collectCoverageFrom": ["src/**/*.js"],
  "transform": {
    ".*": "./node_modules/babel-jest"
  },
  "testRegex": "src/.*\\.test\\.js$",
  "testPathIgnorePatterns": ["src/jspm_packages/", "src/.*.scss$", "src/.*.json$"],
  "rootDir": "./",
  "modulePaths": ["./"],
  "moduleNameMapper": {
    "^.+\\.(css|scss|scss!|json|json!)$": "identity-obj-proxy"
  }
}
